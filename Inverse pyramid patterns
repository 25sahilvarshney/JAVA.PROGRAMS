package PATTERNS;
import java.util.Scanner;
public class inverseprymaidpattern {

	public static void main(String[] args)
	{ 
		Scanner obj = new Scanner(System.in);
		System.out.println("Enter the number:");
		int n = obj.nextInt();
		for(int i = 1;i<=n;i++)
		{
			for(int j = 1;j<=i;j++)
			{
				System.out.print(" ");
			}
			for(int x=1;x<n-i;x++) 
			{
			  System.out.print(x + " ");
			}
			System.out.println();
		}

	}

}
